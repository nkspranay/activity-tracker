{
    "manifest_version": 3,
    "name": "Activity Trackeer",
    "version": "1.0",
    "description": "Tracks your browsing activity",
    "permissions": [
      "tabs",
      "storage",
      "idle"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    }
  }